{
    "builds": [
        {
            "src": "vasundharaNGO/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
        },
        {
          "src": "staticfiles_build/(.*)",
          "use": "@vercel/static"
        },
        {
          "src": "static/**",
          "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "{% static '(.*)' %}",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vasundharaNGO/wsgi.py"
        }
    ]
}
